<!-- Row -->
<div class="row">
  <!-- Sidebar Column -->
  <%= render 'admin/main/sidebar' %>
  <!-- Sidebar Column -->

  <!-- Timeline Column -->
  <div class="col-lg-8 col-xlg-9 col-md-7">
    <div class="card">
      <!-- Nav tabs -->
      <ul class="nav nav-tabs profile-tab" role="tablist">
        <li class="nav-item"> <a class="nav-link active" data-toggle="tab" href="#home" role="tab">Timeline (Coming Soon)</a> </li>
        <li class="nav-item"> <a class="nav-link" data-toggle="tab" href="#profile" role="tab">Profile</a> </li>
      </ul>
      <!-- Tab panes -->
      <div class="tab-content">
        <!-- timeline tab -->
        <div class="tab-pane active" id="home" role="tabpanel">
          <div class="card-body">
            <div class="profiletimeline">
              <%= render 'admin/main/timelines/show_off' %>
              <hr>
              <%= render 'admin/main/timelines/achievement' %>
              <hr>
              <%= render 'admin/main/timelines/status' %>
              <hr>
              <%= render 'admin/main/timelines/quote' %>
            </div>
          </div>
        </div>
        <!--profile tab-->
        <%= render 'admin/main/profile' %>
      </div>
    </div>
  </div>
  <!-- Timeline Column -->
</div>
<!-- Row -->
